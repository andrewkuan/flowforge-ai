    {
      "id": "function-node",
      "name": "Parse Event Details",
      "type": "n8n-nodes-base.function",
      "typeVersion": 1,
      "position": [500, 300],
      "parameters": {
        "functionCode": "// Extract event details from Telegram message\nconst message = items[0].json.message.text;\nconst chatId = items[0].json.message.chat.id;\n\n// Parse event title, date, time from message\nconst eventMatch = message.match(/create event (.+) on (.+) at (.+)/i);\n\nif (eventMatch) {\n  const [, title, date, time] = eventMatch;\n  return [{\n    json: {\n      title: title.trim(),\n      date: date.trim(),\n      time: time.trim(),\n      chatId: chatId,\n      originalMessage: message\n    }\n  }];\n} else {\n  return [{\n    json: {\n      error: \"Invalid format. Use: create event [title] on [date] at [time]\",\n      chatId: chatId\n    }\n  }];\n}"
      }
    },
    {
      "id": "google-calendar-node",
      "name": "Create Calendar Event",
      "type": "n8n-nodes-base.googleCalendar",
      "typeVersion": 1,
      "position": [750, 200],
      "parameters": {
        "operation": "create",
        "calendarId": "primary",
        "title": "={{ $json.title }}",
        "start": "={{ $json.date }}T{{ $json.time }}:00",
        "end": "={{ $json.date }}T{{ $json.time }}:00"
      },
      "credentials": {
        "googleCalendarOAuth2": {
